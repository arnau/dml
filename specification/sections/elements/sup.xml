<?xml version="1.0" encoding="utf-8"?>
<section xml:lang="en" xml:id="element-sup" class="inline"
	xmlns="http://purl.oclc.org/NET/dml/1.0/"
	xmlns:pml="http://purl.oclc.org/NET/pml/1.0/"
	xmlns:dct="http://purl.org/dc/terms/">

	<title>The <pml:node role="element">sup</pml:node> element</title>
	<metadata>
		<list>
			<item property="dct:description">A superscript.</item>
		</list>
	</metadata>
	<p>The <pml:node role="element">sup</pml:node> element represents a superscript.</p>
	<list>
		<item>
			<title>Flow</title>
			<p href="#inline-flow">Inline</p>
		</item>
		<item>
			<title>Children</title>
			<pml:code language="xpath">
( $inline | text() )+
			</pml:code>
		</item>
		<item>
			<title>Attributes</title>
			<pml:code language="xpath">
( $core.attrs* | $meta.attrs* )
			</pml:code>
		</item>
		<item>
			<title>Parents</title>
			<pml:code language="xpath">
( $block | $inline )
			</pml:code>
		</item>
	</list>

</section>